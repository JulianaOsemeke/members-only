<table class="table">

    <thead class = "table">
      <tr>
        <th>UserName</th>
        <th>UserEmail</th>
        <th>Title</th>
        <th>Post</th>
      </tr>
    </thead>
    <tbody>

      <% @posts.each do |post| %>
        <tr>
        <td><%= post.user.username %><td>
        <td><%= post.user.email %></td>
          <td><%= post.title %></td>
          <td><%= post.body %></td>
          
        </tr>
      <% end %>
      
    </tbody>
  </table>



  